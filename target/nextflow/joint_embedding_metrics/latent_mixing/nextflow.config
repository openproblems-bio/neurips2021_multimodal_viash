  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    latent_mixing__input_prediction = "viash_no_value"
    latent_mixing__input_solution = "viash_no_value"
    latent_mixing__output = "viash_no_value"
    latent_mixing__n_neighbors = "100"
    id = ""
    testScript = "test_metric.py"
    testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_latent_mixing.tsv" ]
    latent_mixing {
      name = "latent_mixing"
      container = "joint_embedding_metrics_latent_mixing"
      containerTag = "1.0.1"
      containerRegistry = "openproblems"
      command = "latent_mixing"
      tests {
        isDefined = true
        testScript = "test_metric.py"
        testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_latent_mixing.tsv" ]
      }
      arguments {
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.latent_mixing__input_prediction}"
          example = "prediction.h5ad"
          description = "Prediction by a method"
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.latent_mixing__input_solution}"
          example = "solution.h5ad"
          description = "Gold standard solution"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.latent_mixing__output}"
          example = "output.h5ad"
          description = "Latent mixing metric measures how well the latent cell representations are mixed between batches relative to the global frequency of batches."
        }
        n_neighbors {
          name = "n_neighbors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.latent_mixing__n_neighbors}"
          dflt = "100"
          description = "Number of neighbors for the entropy_batch_mixing metric."
        }
      }
    }
  }