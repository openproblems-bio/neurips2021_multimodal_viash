  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    graph_connectivity__output = "viash_no_value"
    graph_connectivity__input_prediction = "viash_no_value"
    graph_connectivity__input_solution = "viash_no_value"
    graph_connectivity__debug = "false"
    id = ""
    testScript = "test_metric.py"
    testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_graph_connectivity.tsv" ]
    graph_connectivity {
      name = "graph_connectivity"
      container = "joint_embedding_metrics_graph_connectivity"
      containerTag = "1.0.0"
      containerRegistry = "openproblems"
      command = "graph_connectivity"
      tests {
        isDefined = true
        testScript = "test_metric.py"
        testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_graph_connectivity.tsv" ]
      }
      arguments {
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.graph_connectivity__output}"
          description = "Metric scores by comparing the prediction to the gold standard solution."
        }
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.graph_connectivity__input_prediction}"
          description = "Prediction by a method"
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.graph_connectivity__input_solution}"
          description = "Gold standard solution."
        }
        debug {
          name = "debug"
          otype = "--"
          required = false
          type = "boolean"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.graph_connectivity__debug}"
          dflt = "false"
          description = "Verbose output for debugging."
        }
      }
    }
  }