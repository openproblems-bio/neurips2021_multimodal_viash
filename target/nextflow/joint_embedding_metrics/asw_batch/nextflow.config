  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    asw_batch__input_prediction = "viash_no_value"
    asw_batch__input_solution = "viash_no_value"
    asw_batch__output = "viash_no_value"
    asw_batch__debug = "false"
    id = ""
    testScript = "test_metric.py"
    testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_asw_batch.tsv" ]
    asw_batch {
      name = "asw_batch"
      container = "joint_embedding_metrics_asw_batch"
      containerTag = "main_build"
      containerRegistry = "openproblems"
      command = "asw_batch"
      tests {
        isDefined = true
        testScript = "test_metric.py"
        testResources = [ "../../unit_tests/test_metric.py", "../../../../resources_test", "metric_meta_asw_batch.tsv" ]
      }
      arguments {
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.asw_batch__input_prediction}"
          example = "prediction.h5ad"
          description = "Prediction by a method"
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.asw_batch__input_solution}"
          example = "solution.h5ad"
          description = "Gold standard solution."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.asw_batch__output}"
          example = "output.h5ad"
          description = "Metric scores by comparing the prediction to the gold standard solution."
        }
        debug {
          name = "debug"
          otype = "--"
          required = false
          type = "boolean"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.asw_batch__debug}"
          dflt = "false"
          description = "Verbose output for debugging."
        }
      }
    }
  }