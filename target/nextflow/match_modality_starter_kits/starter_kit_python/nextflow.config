  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    python_starter_kit__input_train_mod1 = "viash_no_value"
    python_starter_kit__input_train_mod2 = "viash_no_value"
    python_starter_kit__input_train_sol = "viash_no_value"
    python_starter_kit__input_test_mod1 = "viash_no_value"
    python_starter_kit__input_test_mod2 = "viash_no_value"
    python_starter_kit__output = "viash_no_value"
    python_starter_kit__distance_method = "minkowski"
    python_starter_kit__n_pcs = "4"
    python_starter_kit__n_neighbors = "5"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "sample_data" ]
    python_starter_kit {
      name = "python_starter_kit"
      container = "match_modality_starter_kits_python_starter_kit"
      containerTag = "1.1.1"
      containerRegistry = "openproblems"
      command = "python_starter_kit"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "sample_data" ]
      }
      arguments {
        input_train_mod1 {
          name = "input_train_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_train_mod1}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled train mod1 profiles."
        }
        input_train_mod2 {
          name = "input_train_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_train_mod2}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled train mod2 profiles."
        }
        input_train_sol {
          name = "input_train_sol"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_train_sol}"
          example = "dataset_solution.h5ad"
          description = "The pairing of train mod1&mod2 profiles."
        }
        input_test_mod1 {
          name = "input_test_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_test_mod1}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled test mod1 profiles."
        }
        input_test_mod2 {
          name = "input_test_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_test_mod2}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled test mod2 profiles."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__output}"
          example = "output.h5ad"
          description = "The predicted pairing of test mod1&mod2 profiles."
        }
        distance_method {
          name = "distance_method"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__distance_method}"
          dflt = "minkowski"
          description = "The distance metric to use. Possible values include `euclidean` and `minkowski`."
        }
        n_pcs {
          name = "n_pcs"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__n_pcs}"
          dflt = "4"
          description = "Number of components to use for dimensionality reduction."
        }
        n_neighbors {
          name = "n_neighbors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__n_neighbors}"
          dflt = "5"
          description = "Number of neighbors to use."
        }
      }
    }
  }