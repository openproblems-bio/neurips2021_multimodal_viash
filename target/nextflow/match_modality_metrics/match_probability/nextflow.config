  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    match_probability__input_prediction = "no_default_value_configured"
    match_probability__input_solution = "viash_no_value"
    match_probability__output = "viash_no_value"
