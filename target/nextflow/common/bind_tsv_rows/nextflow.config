  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    bind_tsv_rows__input = "viash_no_value"
    bind_tsv_rows__output = "viash_no_value"
    id = ""
    testScript = ""
    testResources = [ "../../../../resources_test" ]
    bind_tsv_rows {
      name = "bind_tsv_rows"
