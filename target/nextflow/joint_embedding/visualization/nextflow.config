  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    visualization__input_prediction = "viash_no_value"
    visualization__input_solution = "viash_no_value"
    visualization__output_umap = "viash_no_value"
    visualization__output_emb = "viash_no_value"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "sample_data" ]
    visualization {
      name = "visualization"
      container = "joint_embedding_visualization"
      containerTag = "1.1.0"
      containerRegistry = "openproblems"
      command = "visualization"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "sample_data" ]
      }
      arguments {
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.visualization__input_prediction}"
          example = "prediction.h5ad"
          description = "Predicted joint embedding."
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.visualization__input_solution}"
          example = "solution.h5ad"
          description = "Ground truth dataset."
        }
        output_umap {
          name = "output_umap"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.visualization__output_umap}"
          example = "umap.png"
          description = "UMAP visualisation of joint embedding."
        }
        output_emb {
          name = "output_emb"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.visualization__output_emb}"
          example = "emb.png"
          description = "Scatterplot of first 2 dimensions of the joint embedding."
        }
      }
    }
  }