  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    python_starter_kit__input_mod1 = "viash_no_value"
    python_starter_kit__input_mod2 = "viash_no_value"
    python_starter_kit__output = "viash_no_value"
    python_starter_kit__n_dim = "50"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "sample_data" ]
    python_starter_kit {
      name = "python_starter_kit"
      container = "joint_embedding_starter_kits_python_starter_kit"
      containerTag = "1.2.0"
      containerRegistry = "openproblems"
      command = "python_starter_kit"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "sample_data" ]
      }
      arguments {
        input_mod1 {
          name = "input_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Modality 1 dataset."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__input_mod2}"
          example = "dataset_mod2.h5ad"
          description = "Modality 2 dataset."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__output}"
          example = "output.h5ad"
          description = "Data for all cells in mod1 and mod2 embedded to â‰¤100 dimensions."
        }
        n_dim {
          name = "n_dim"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.python_starter_kit__n_dim}"
          dflt = "50"
          description = "Number of dimensions to use for dimensionality reduction."
        }
      }
    }
  }