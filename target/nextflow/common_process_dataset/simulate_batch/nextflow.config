  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    simulate_batch__input_rna = "viash_no_value"
    simulate_batch__input_mod2 = "viash_no_value"
    simulate_batch__output_rna = "viash_no_value"
    simulate_batch__output_mod2 = "viash_no_value"
    simulate_batch__num_batches = "2"
    simulate_batch__batch_sd = "0.4"
    id = ""
    testScript = ""
    testResources = [  ]
    simulate_batch {
      name = "simulate_batch"
      container = "common_process_dataset_simulate_batch"
      containerTag = "1.1.1"
      containerRegistry = "openproblems"
      command = "simulate_batch"
      tests {
        isDefined = false
        testScript = "NA"
        testResources = [  ]
      }
      arguments {
        input_rna {
          name = "input_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__input_rna}"
          example = "input.h5ad"
          description = "Raw dataset in a h5ad file."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__input_mod2}"
          example = "input.h5ad"
          description = "Raw dataset in a h5ad file."
        }
        output_rna {
          name = "output_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__output_rna}"
          example = "output.h5ad"
          description = "Normalized h5ad file."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__output_mod2}"
          example = "output.h5ad"
          description = "Normalized h5ad file."
        }
        num_batches {
          name = "num_batches"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__num_batches}"
          dflt = "2"
          description = "Number of batches to simulate when dataset doesn't contain any batches."
        }
        batch_sd {
          name = "batch_sd"
          otype = "--"
          required = false
          type = "double"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.simulate_batch__batch_sd}"
          dflt = "0.4"
          description = "Noise generator for generating the batch effects."
        }
      }
    }
  }