  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    baseline_knnr_r__input_train_mod1 = "viash_no_value"
    baseline_knnr_r__input_test_mod1 = "viash_no_value"
    baseline_knnr_r__input_train_mod2 = "viash_no_value"
    baseline_knnr_r__output = "viash_no_value"
    baseline_knnr_r__distance_method = "spearman"
    baseline_knnr_r__n_pcs = "50"
    baseline_knnr_r__n_neighbors = "20"
    id = ""
    testScript = "test_method.R"
    testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
    baseline_knnr_r {
      name = "baseline_knnr_r"
      container = "predict_modality_methods_baseline_knnr_r"
      containerTag = "1.3.0"
      containerRegistry = "openproblems"
      command = "baseline_knnr_r"
      tests {
        isDefined = true
        testScript = "test_method.R"
        testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
      }
      arguments {
        input_train_mod1 {
          name = "input_train_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__input_train_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Censored dataset, training cells."
        }
        input_test_mod1 {
          name = "input_test_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__input_test_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Censored dataset, test cells."
        }
        input_train_mod2 {
          name = "input_train_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__input_train_mod2}"
          example = "dataset_mod2.h5ad"
          description = "Censored dataset."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__output}"
          example = "output.h5ad"
          description = "Dataset with predicted values for modality2."
        }
        distance_method {
          name = "distance_method"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__distance_method}"
          dflt = "spearman"
          description = "The distance method to use. Possible values are euclidean, pearson, spearman and others."
        }
        n_pcs {
          name = "n_pcs"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__n_pcs}"
          dflt = "50"
          description = "Number of principal components to use."
        }
        n_neighbors {
          name = "n_neighbors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_knnr_r__n_neighbors}"
          dflt = "20"
          description = "Number of neighbors to use in the knn regression."
        }
      }
    }
  }