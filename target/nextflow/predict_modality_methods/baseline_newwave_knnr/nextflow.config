  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    baseline_newwave_knnr__input_train_mod1 = "viash_no_value"
    baseline_newwave_knnr__input_train_mod2 = "viash_no_value"
    baseline_newwave_knnr__input_test_mod1 = "viash_no_value"
    baseline_newwave_knnr__newwave_maxiter = "40"
    baseline_newwave_knnr__newwave_ngene = "200"
    baseline_newwave_knnr__newwave_ncell = "200"
    baseline_newwave_knnr__n_neighbors = "20"
    baseline_newwave_knnr__output = "viash_no_value"
    id = ""
    testScript = "test_method.R"
    testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
    baseline_newwave_knnr {
      name = "baseline_newwave_knnr"
      container = "predict_modality_methods_baseline_newwave_knnr"
      containerTag = "1.3.0"
      containerRegistry = "openproblems"
      command = "baseline_newwave_knnr"
      tests {
        isDefined = true
        testScript = "test_method.R"
        testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
      }
      arguments {
        input_train_mod1 {
          name = "input_train_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__input_train_mod1}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled train mod1 profiles."
        }
        input_train_mod2 {
          name = "input_train_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__input_train_mod2}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled train mod2 profiles."
        }
        input_test_mod1 {
          name = "input_test_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__input_test_mod1}"
          example = "dataset_censored.h5ad"
          description = "The censored shuffled test mod1 profiles."
        }
        newwave_maxiter {
          name = "newwave_maxiter"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__newwave_maxiter}"
          dflt = "40"
          description = "Maximum number of NewWave iterations."
        }
        newwave_ngene {
          name = "newwave_ngene"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__newwave_ngene}"
          dflt = "200"
          description = "Setting of the n_gene_par NewWave parameter."
        }
        newwave_ncell {
          name = "newwave_ncell"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__newwave_ncell}"
          dflt = "200"
          description = "Setting of the n_cell_par NewWave parameter."
        }
        n_neighbors {
          name = "n_neighbors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__n_neighbors}"
          dflt = "20"
          description = "Number of neighbors to use in the knn regression."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave_knnr__output}"
          example = "output.h5ad"
          description = "The predicted pairing of test mod1&mod2 profiles."
        }
      }
    }
  }