  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    baseline_newwave__input_mod1 = "viash_no_value"
    baseline_newwave__input_mod2 = "viash_no_value"
    baseline_newwave__output = "viash_no_value"
    baseline_newwave__maxiter = "100"
    baseline_newwave__k = "10"
    baseline_newwave__hvg_sel = "1000"
    id = ""
    testScript = "test_method.R"
    testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
    baseline_newwave {
      name = "baseline_newwave"
      container = "joint_embedding_methods_baseline_newwave"
      containerTag = "1.3.0"
      containerRegistry = "openproblems"
      command = "baseline_newwave"
      tests {
        isDefined = true
        testScript = "test_method.R"
        testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
      }
      arguments {
        input_mod1 {
          name = "input_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__input_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Censored dataset."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__input_mod2}"
          example = "dataset_mod2.h5ad"
          description = "Censored dataset."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__output}"
          example = "output.h5ad"
          description = "The predicted pairing of test mod1&mod2 profiles."
        }
        maxiter {
          name = "maxiter"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__maxiter}"
          dflt = "100"
          description = "Maximum number of NewWave iterations."
        }
        k {
          name = "k"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__k}"
          dflt = "10"
          description = "NewWave K parameter."
        }
        hvg_sel {
          name = "hvg_sel"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_newwave__hvg_sel}"
          dflt = "1000"
          description = "Number of features per modality to use."
        }
      }
    }
  }