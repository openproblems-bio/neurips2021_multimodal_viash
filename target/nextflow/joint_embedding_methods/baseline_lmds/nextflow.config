  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    baseline_lmds__input_mod1 = "viash_no_value"
    baseline_lmds__input_mod2 = "viash_no_value"
    baseline_lmds__output = "viash_no_value"
    baseline_lmds__distance_method = "spearman"
    baseline_lmds__n_dims = "10"
    id = ""
    testScript = "test_method.R"
    testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
    baseline_lmds {
      name = "baseline_lmds"
      container = "joint_embedding_methods_baseline_lmds"
      containerTag = "1.0.2"
      containerRegistry = "openproblems"
      command = "baseline_lmds"
      tests {
        isDefined = true
        testScript = "test_method.R"
        testResources = [ "../../unit_tests/test_method.R", "../../../../resources_test" ]
      }
      arguments {
        input_mod1 {
          name = "input_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_lmds__input_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Censored dataset."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_lmds__input_mod2}"
          example = "dataset_mod2.h5ad"
          description = "Censored dataset."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_lmds__output}"
          example = "output.h5ad"
          description = "Dataset with predicted values for modality2."
        }
        distance_method {
          name = "distance_method"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_lmds__distance_method}"
          dflt = "spearman"
          description = "The distance method to use. Possible values are euclidean, pearson, spearman and others."
        }
        n_dims {
          name = "n_dims"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_lmds__n_dims}"
          dflt = "10"
          description = "Number of dimensions to output."
        }
      }
    }
  }