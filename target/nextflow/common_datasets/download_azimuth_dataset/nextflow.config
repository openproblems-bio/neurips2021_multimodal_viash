  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    download_azimuth_dataset__id = "viash_no_value"
    download_azimuth_dataset__input_count = "viash_no_value"
    download_azimuth_dataset__input_meta = "viash_no_value"
    download_azimuth_dataset__organism = "viash_no_value"
    download_azimuth_dataset__output_rna = "viash_no_value"
    download_azimuth_dataset__output_mod2 = "viash_no_value"
    id = ""
    testScript = ""
    testResources = [  ]
    download_azimuth_dataset {
      name = "download_azimuth_dataset"
      container = "common_datasets_download_azimuth_dataset"
      containerTag = "1.0.2"
      containerRegistry = "openproblems"
      command = "download_azimuth_dataset"
      tests {
        isDefined = false
        testScript = "NA"
        testResources = [  ]
      }
      arguments {
        id {
          name = "id"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__id}"
          example = "azimuth_ref"
          description = "The id of the output dataset id"
        }
        input_count {
          name = "input_count"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__input_count}"
          example = "https://www.ncbi.nlm.nih.gov/geo/download/?acc=GSE164378&format=file"
          description = "Location URL to the GEO raw counts tar file"
        }
        input_meta {
          name = "input_meta"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__input_meta}"
          example = "https://www.ncbi.nlm.nih.gov/geo/download/?acc=GSE164378&format=file&file=GSE164378%5Fsc%2Emeta%2Edata%5F3P%2Ecsv%2Egz"
          description = "Location URL to the GEO meta data file"
        }
        organism {
          name = "organism"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__organism}"
          example = "human"
          description = "The organism of the dataset"
        }
        output_rna {
          name = "output_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__output_rna}"
          example = "output_rna.h5ad"
          description = "Output h5ad RNA file."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_azimuth_dataset__output_mod2}"
          example = "output_mod2.h5ad"
          description = "Output h5ad modality2 file (ATAC or Antibody capture)."
        }
      }
    }
  }