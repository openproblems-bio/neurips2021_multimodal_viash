  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    download_totalvi_10x_dataset__id = "viash_no_value"
    download_totalvi_10x_dataset__input = "viash_no_value"
    download_totalvi_10x_dataset__organism = "viash_no_value"
    download_totalvi_10x_dataset__output_rna = "viash_no_value"
    download_totalvi_10x_dataset__output_mod2 = "viash_no_value"
    id = ""
    testScript = ""
    testResources = [  ]
    download_totalvi_10x_dataset {
      name = "download_totalvi_10x_dataset"
      container = "common_datasets_download_totalvi_10x_dataset"
      containerTag = "1.0.2"
      containerRegistry = "openproblems"
      command = "download_totalvi_10x_dataset"
      tests {
        isDefined = false
        testScript = "NA"
        testResources = [  ]
      }
      arguments {
        id {
          name = "id"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_totalvi_10x_dataset__id}"
          example = "totalVI_10x_malt_10k"
          description = "The id of the output dataset id"
        }
        input {
          name = "input"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_totalvi_10x_dataset__input}"
          example = "https://github.com/YosefLab/totalVI_reproducibility/raw/master/data/spleen_lymph_111.h5ad"
          description = "Location URL to an h5ad file from the totalVI reproducibility github."
        }
        organism {
          name = "organism"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_totalvi_10x_dataset__organism}"
          example = "human"
          description = "The organism of the dataset"
        }
        output_rna {
          name = "output_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_totalvi_10x_dataset__output_rna}"
          example = "output_rna.h5ad"
          description = "Output h5ad RNA file."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_totalvi_10x_dataset__output_mod2}"
          example = "output_mod2.h5ad"
          description = "Output h5ad modality2 file (ATAC or Antibody capture)."
        }
      }
    }
  }