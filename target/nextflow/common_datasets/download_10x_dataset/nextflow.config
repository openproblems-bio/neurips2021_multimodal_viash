  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    download_10x_dataset__id = "viash_no_value"
    download_10x_dataset__input = "viash_no_value"
    download_10x_dataset__organism = "viash_no_value"
    download_10x_dataset__output_rna = "viash_no_value"
    download_10x_dataset__output_mod2 = "viash_no_value"
    id = ""
    testScript = "test.R"
    testResources = [ "test.R" ]
    download_10x_dataset {
      name = "download_10x_dataset"
      container = "common_datasets_download_10x_dataset"
      containerTag = "1.1.0"
      containerRegistry = "openproblems"
      command = "download_10x_dataset"
      tests {
        isDefined = true
        testScript = "test.R"
        testResources = [ "test.R" ]
      }
      arguments {
        id {
          name = "id"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__id}"
          example = "dyngen_dataset"
          description = "The id of the output dataset id"
        }
        input {
          name = "input"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__input}"
          example = "https://cf.10xgenomics.com/samples/cell-exp/3.0.0/pbmc_1k_protein_v3/pbmc_1k_protein_v3_raw_feature_bc_matrix.h5"
          description = "Location URL to an h5 file from the 10x genomics website."
        }
        organism {
          name = "organism"
          otype = "--"
          required = true
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__organism}"
          example = "human"
          description = "The organism of the dataset"
        }
        output_rna {
          name = "output_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__output_rna}"
          example = "output_rna.h5ad"
          description = "Output h5ad RNA file."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.download_10x_dataset__output_mod2}"
          example = "output_mod2.h5ad"
          description = "Output h5ad modality2 file (ATAC or Antibody capture)."
        }
      }
    }
  }